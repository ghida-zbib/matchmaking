package bucky;

import java.util.Scanner;

class apples {
   public static void main (String args[])
   {
	   Scanner Adding=new Scanner(System.in);
	   int turn=0;
	   double total=0.0;
	   while (turn<10)
	   {
		   System.out.println("What number do you want to add to the average? ");
		   total+=Adding.nextDouble();
		   turn++;
	   }
	   double finalTotal=total/10;
	   System.out.println(finalTotal);
   }
}
