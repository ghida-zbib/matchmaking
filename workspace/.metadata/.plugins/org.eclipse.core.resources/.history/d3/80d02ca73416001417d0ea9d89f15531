package bucky;

class apples {
	public static void change(int x[])
	{
		for (int counter=0; counter<x.length; counter++)
		{
			x[counter]+=5;
		}
	}
	public void main (String args[])
	{
	   int bucky[]={3,4,5,6,7};
	   apples.change(bucky);
	   for (int y: bucky)
	   {
		   System.out.println(y);
	   }
	}
}
