import java.util.Scanner;

public class Decider {
	//define global variables
	static String firstBloodType; //Recipient Blood type
	static String secondBloodType; //Donor Blood Type
	static int fastFood; //amount of fast food eaten
	static String vegetarianOrVegan; //Vegan or vegetarian?
	static String oilAndButter; //amount of oil
	static int eggs; //how many eggs per week
	static int highFatDesserts; //how many fat desserts per week
	
	static int kidneyCompatability; //FINAL RETURNED INT, HIGHER NUMBER=MORE COMPATABLE
	
	//((((IMPORTANT)))) CONSTRUCTOR TAKES INPUTS IN ORDER
	public Decider (String DonorBlood, String RecipientBlood, int amountFastFood, String vegOrVeg, String oAndB, int huevos, int fatDesserts)
	{
		firstBloodType=DonorBlood;
		secondBloodType=RecipientBlood;
		fastFood=amountFastFood;
		vegetarianOrVegan=vegOrVeg;
		oilAndButter=oAndB;
		eggs=huevos;
		highFatDesserts=fatDesserts;
	}
	
	//find compatability based on blood type
	public static void stringToBlood (String firstBloodType, String secondBloodType)
	{
		if ((firstBloodType=="O+") && (secondBloodType=="O+"))
		{
			kidneyCompatability+=90;
		}
		else if ((firstBloodType=="O+") && (secondBloodType=="O-"))
		{
			kidneyCompatability+=45;
		}
		else if ((firstBloodType=="O+"))
		{
			kidneyCompatability+=10;
		}
		else if ((firstBloodType=="O-") && (secondBloodType=="O-"))
		{
			kidneyCompatability+=90;
		}
		else if ((firstBloodType=="O-") && (secondBloodType=="O+"))
		{
			kidneyCompatability+=45;
		}
		else if ((firstBloodType==("O-")))
		{
			kidneyCompatability+=10;
		}
		else if ((firstBloodType.equals("A+")) && (secondBloodType.equals("A+") || secondBloodType.equals("O+")))
		{
			kidneyCompatability+=90;
		}
		else if ((firstBloodType.equals("A+")) && (secondBloodType.equals("A-") || secondBloodType.equals("O-")))
		{
			kidneyCompatability+=45;
		}
		else if (firstBloodType.equals("A+"))
		{
			kidneyCompatability+=10;
		}
		else if ((firstBloodType.equals("B+")) && (secondBloodType.equals("B+") || secondBloodType.equals("O+")))
		{
			kidneyCompatability+=90;
		}
		else if ((firstBloodType.equals("B+")) && (secondBloodType.equals("B-") || secondBloodType.equals("O-")))
		{
			kidneyCompatability+=45;
		}
		else if(firstBloodType.equals("B+"))
		{
			kidneyCompatability+=10;
		}
		else if ((firstBloodType.equals("B-")) && (secondBloodType.equals("B-") || secondBloodType.equals("O-")))
		{
			kidneyCompatability+=90;
		}
		else if ((firstBloodType.equals("B-")) && (secondBloodType.equals("B+") || secondBloodType.equals("O+")))
		{
			kidneyCompatability+=45;
		}
		else if (firstBloodType.equals("B-"))
		{
			kidneyCompatability+=90;
		}
		else if ((firstBloodType.equals("AB+")) && (secondBloodType.equals("A+") || secondBloodType.equals("B+") || secondBloodType.equals("O+") || secondBloodType.equals("AB+")))
		{
			kidneyCompatability+=90;
		}
		else if ((firstBloodType.equals("AB+")) && (secondBloodType.equals("A-") || secondBloodType.equals("B-") || secondBloodType.equals("O-") || secondBloodType.equals("AB-")))
		{
			kidneyCompatability+=45;
		}
		else if ((firstBloodType.equals("AB-")) && (secondBloodType.equals("A-") || secondBloodType.equals("B-") || secondBloodType.equals("O-") || secondBloodType.equals("AB-")))
		{
			kidneyCompatability+=90;
		}
		else if ((firstBloodType.equals("AB-")) && (secondBloodType.equals("A+") || secondBloodType.equals("B+") || secondBloodType.equals("O+") || secondBloodType.equals("AB+")))
		{
			kidneyCompatability+=45;
		}
		else
		{
			kidneyCompatability+=0;
		}
	}
	//find dietary health of donor
	public static void fastFood (int fastFood)
	{
		if (fastFood==0)
		{
			kidneyCompatability+=3;
		}
		else if ((fastFood>=1) && (fastFood<=3))
		{
			kidneyCompatability+=0;
		}
		else if ((fastFood>=4) && (fastFood<=6))
		{
			kidneyCompatability+=(-1);
		}
		else if ((fastFood>=7) && (fastFood<=9))
		{
			kidneyCompatability+=(-4);
		}
		else
		{
			kidneyCompatability+=(-7);
		}
	}
	public static void vegetarianOrVegan (String vegetarianOrVegan)
	{
		if (vegetarianOrVegan.equals("Vegatarian") || vegetarianOrVegan.equals("vegatarian"))
		{
			kidneyCompatability+=8;
		}
		else if (vegetarianOrVegan.equals("Vegan") || vegetarianOrVegan.equals("vegan"))
		{
			kidneyCompatability+=9;
		}
		else
		{
			kidneyCompatability+=0;
		}
	}
	public static void oilAndButter (String oilAndButter)
	{
		if (oilAndButter.equals("Rarely") || oilAndButter.equals("rarely"))
		{
			kidneyCompatability+=3;
		}
		if (oilAndButter.equals("Sometimes") || oilAndButter.equals("sometimes"))
		{
			kidneyCompatability+=0;
		}
		if (oilAndButter.equals("Often") || oilAndButter.equals("often"))
		{
			kidneyCompatability-=(-3);
		}
		else
		{
			kidneyCompatability+=(-6);
		}
	}
	public static void eggs (int eggs)
	{
		if (eggs==0)
		{
			kidneyCompatability+=2;
		}
		else if ((eggs>=1) && (eggs<=4))
		{
			kidneyCompatability+=0;
		}
		else if ((eggs>=5) && (eggs<=8))
		{
			kidneyCompatability+=(-2);
		}
		else
		{
			kidneyCompatability+=(-4);
		}
	}
	public static void highFatDesserts (int highFatDesserts)
	{
		if (highFatDesserts==0)
		{
			kidneyCompatability+=0;
		}
		else if ((highFatDesserts>=1) && (highFatDesserts<=3))
		{
			kidneyCompatability+=(-2);
		}
		else if ((highFatDesserts>=4) && (highFatDesserts<=6))
		{
			kidneyCompatability+=(-4);
		}
		else if ((highFatDesserts>=1) && (highFatDesserts<=3))
		{
			kidneyCompatability+=(-2);
		}
	}	
	
	//Combines to find the compatability of two different blood types/diets into an int
	public static void main (String args[])
	{
		Decider.highFatDesserts(highFatDesserts);
		Decider.eggs(eggs);
		Decider.stringToBlood(firstBloodType, secondBloodType);
		Decider.oilAndButter(oilAndButter);
		Decider.fastFood(fastFood);
		Decider.vegetarianOrVegan(vegetarianOrVegan);
		System.out.println(kidneyCompatability);	
	}
}
